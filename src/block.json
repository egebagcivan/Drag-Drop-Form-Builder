{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/jotform-form-builder",
	"version": "0.1.0",
	"title": "Jotform Form Builder",
	"category": "widgets",
	"description": "Jotform Drag & Drop Form Builder",
	"attributes": {
		"message": {
			"type": "string",
			"default": "Select a form to embed in your site."
		},
		"isModalOpen":{
			"type":"boolean",
			"default" : false
		},
		"isVisible":{
			"type":"boolean",
			"default" : true
		},

		"formData":{
			"type":"object",
			"default":{
				"id":null,
				"title":null
			}
		},
		"alignment":{
			"type":"string",
			"default":"none"
		},
		"maxWidth":{
			"type":"string",
			"default":"650px"
		},
		"heightStyle":{
			"type":"number",
			"default":160
		},
		"heightMetric":{
			"type":"string",
			"default":"px"
		},


		"alignmentStyle":{
			"type":"object",
			"default": {
				"maxWidth": "650px",
				"minWidth": 300,
				"width":650
			}
		},
		"editable": {
			"type":"boolean",
			"default" : true
		},
		"style":{
			"type":"object",
			"default":{
				"fontWeight": "lighter"
			}
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "jotform-form-builder",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
